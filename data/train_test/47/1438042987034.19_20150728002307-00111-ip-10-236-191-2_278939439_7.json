            "\u0625\u0633\u0645 \u0627\u0644\u0645\u0644\u0641",
            "\u0627\u0639\u062a\u0642\u0627\u062f \u0623\u0647\u0644 \u0627\u0644\u0623\u062b\u0631 \u0641\u064a \u0627\u0644\u0645\u0647\u062f\u064a \u0627\u0644\u0645\u0646\u062a\u0638\u0631 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0627\u0644\u0628\u064a\u0639\u0629 \u0644\u0648\u0644\u064a \u0627\u0644\u0623\u0645\u0631: \u0633\u0646\u0646 \u0648\u0645\u062e\u0627\u0644\u0641\u0627\u062a - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0627\u0644\u062a\u0630\u0643\u0631\u0629 \u0628\u064a\u0648\u0645 \u0627\u0644\u0642\u064a\u0627\u0645\u0629 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f [\u0645\u0631\u0626\u064a]",
            "\u0627\u0644\u062d\u0645\u0644\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0645\u064a\u0629 \u0636\u062f \u0627\u0644\u062f\u0639\u0648\u0629 \u0627\u0644\u0625\u0635\u0644\u0627\u062d\u064a\u0629 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0628\u064a\u0627\u0646 \u0627\u0644\u062e\u0637\u0631 \u0641\u064a \u0639\u0627\u0642\u0628\u0629 \u0627\u0644\u0646\u0638\u0631 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u062a\u0646\u0628\u064a\u0647 \u0627\u0644\u0625\u0646\u0633\u0627\u0646 \u0628\u062e\u0637\u0631 \u0627\u0644\u062f\u062e\u0627\u0646 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u062a\u0648\u062c\u064a\u0647\u0627\u062a \u062d\u0648\u0644 \u0627\u0644\u0645\u062e\u062f\u0631\u0627\u062a \u0648\u0627\u0644\u0645\u0633\u0643\u0631\u0627\u062a - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u062d\u0642\u0627\u0626\u0642 \u0645\u0647\u0645\u0629 \u062d\u0648\u0644 \u0627\u0644\u062a\u0628\u0634\u064a\u0631 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u062d\u0642\u0648\u0642 \u0627\u0644\u0648\u0627\u0644\u062f\u064a\u0646 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0634\u0631 \u0642\u062a\u0644\u0649 \u062a\u062d\u062a \u0623\u062f\u064a\u0645 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0635\u0644\u0627\u0629 \u0627\u0644\u062a\u0631\u0627\u0648\u064a\u062d + \u062f\u0631\u0633 \u0641\u064a \u0627\u0644\u0633\u064a\u0631\u0629 \u0627\u0644\u0646\u0628\u0648\u064a\u0629 01 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0635\u0644\u0627\u0629 \u0627\u0644\u062a\u0631\u0627\u0648\u064a\u062d + \u062f\u0631\u0633 \u0641\u064a \u0627\u0644\u0633\u064a\u0631\u0629 \u0627\u0644\u0646\u0628\u0648\u064a\u0629 02 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0635\u0644\u0627\u0629 \u0627\u0644\u062a\u0631\u0627\u0648\u064a\u062d + \u062f\u0631\u0633 \u0641\u064a \u0627\u0644\u0633\u064a\u0631\u0629 \u0627\u0644\u0646\u0628\u0648\u064a\u0629 03 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0639\u0628\u0631 \u0645\u0646 \u0633\u064a\u0631 \u0623\u0647\u0644 \u0627\u0644\u0623\u062b\u0631 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f [\u0645\u0631\u0626\u064a]",
            "\u0639\u0642\u064a\u062f\u0629 \u0627\u0644\u0625\u0645\u0627\u0645 \u0645\u062d\u0645\u062f \u0628\u0646 \u0639\u0628\u062f\u0627\u0644\u0648\u0647\u0627\u0628 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0639\u0642\u064a\u062f\u062f\u0629 \u0623\u0647\u0644 \u0627\u0644\u0633\u0646\u0629 \u0641\u064a \u0627\u0644\u0643\u0631\u0627\u0645\u0627\u062a \u0648 \u0627\u0644\u0623\u0648\u0644\u064a\u0627\u0621 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0639\u0644\u0627\u0645\u0627\u062a \u0627\u0644\u0633\u0627\u0639\u0629 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f [\u0645\u0631\u0626\u064a]",
            "\u0641\u062a\u0646\u0629 \u0627\u0644\u0645\u0633\u064a\u062d \u0627\u0644\u062f\u062c\u0627\u0644 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0642\u0635\u0629 \u0639\u0628\u062f\u0629 \u0628\u0646 \u0639\u0628\u062f\u0627\u0644\u0631\u062d\u064a\u0645 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f [\u0645\u0642\u0637\u0639 \u0635\u0648\u062a\u064a]",
            "\u0645\u0632\u0627\u064a\u0627 \u062f\u0639\u0648\u0629 \u0627\u0644\u0625\u0645\u0627\u0645 \u0645\u062d\u0645\u062f \u0628\u0646 \u0639\u0628\u062f \u0627\u0644\u0648\u0647\u0627\u0628 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0645\u0648\u0627\u0642\u0641 \u0645\u0646 \u0627\u0644\u0633\u064a\u0631\u0629 \u0627\u0644\u0646\u0628\u0648\u064a\u0629 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f [\u0645\u0631\u0626\u064a]",
            "\u0646\u0648\u0627\u0642\u0636 \u0627\u0644\u062a\u0648\u062d\u064a\u062f - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f",
            "\u0647\u0630\u0647 \u0639\u0642\u064a\u062f\u0629 \u0627\u0644\u0625\u0645\u0627\u0645 \u0645\u062d\u0645\u062f \u0628\u0646 \u0639\u0628\u062f \u0627\u0644\u0648\u0647\u0627\u0628 - \u0627\u0644\u0634\u064a\u062e \u0633\u0644\u0637\u0627\u0646 \u0627\u0644\u0639\u064a\u062f"
